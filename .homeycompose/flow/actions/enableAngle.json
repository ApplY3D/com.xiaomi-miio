{
  "id": "enableAngle",
  "title": {
    "en": "Enable Fan Oscillating",
    "nl": "Instellen ventilator oscillatie"
  },
  "titleFormatted": {
    "en": "[[angle]] Fan Oscillating",
    "nl": "Ventilator oscillatie [[angle]]"
